69. Sqrt(x)

/*
Implement int sqrt(int x).

Compute and return the square root of x.
*/

Solution 1:
class Solution {
public:
    int mySqrt(int x) {
        if(x==0)
            return 0;
        int left=1,right=x,ans;
        while(left<=right){
            int mid = left + (right - left) / 2;
            if (mid <= x / mid) {
                left = mid + 1;
                ans = mid;
            } else {
                right = mid - 1;
            }
        }
        return ans;
    }
};


Solution 2:
class Solution {
public:
    int mySqrt(int x) {
        long low=0,high=x,mid,pro;
        while(low<=high){
            mid=low+(high-low)/2;
            pro=mid*mid;
            if(pro==x)
                return mid;
            else if(pro>x)
                high=mid-1;
            else
                low=mid+1;
        }
        return low-1;
    }
};
